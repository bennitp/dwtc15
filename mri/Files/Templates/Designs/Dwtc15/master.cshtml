													<li id="minicart">
														@MiniCart()
													</li>
												</ul>
											</li>
											}
										</ul>
									</nav>
								</div>
							</div>
						</div>
					</div>
				}

				<!-- MAIN NAV -->

                @{
                	var sticky = GeneralSettings.Navigation.StickyMenu;
                	var navbarpos = GeneralSettings.Navigation.Position;
                	var selectionstyle = GeneralSettings.Navigation.SelectionStyle;
                }

